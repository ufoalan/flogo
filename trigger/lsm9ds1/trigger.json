    {
      "name": "accZ",
      "type": "integer"
    },
    {
      "name": "gyrX",
      "type": "integer"
    },
    {
      "name": "gyrY",
      "type": "integer"
    },
    {
      "name": "gyrZ",
      "type": "integer"
    },
    {
      "name": "magX",
      "type": "integer"
    },
    {
      "name": "magY",
      "type": "integer"
    },
    {
      "name": "magZ",
      "type": "integer"
    }
  ],
  "handler": {
    "settings": [
      {
        "name": "handler_setting",
        "type": "string"
